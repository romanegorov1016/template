{
  "name": "mercer-testframework-wdio-template",
  "version": "2.0.0",
  "description": "End-to-end test automation framework starter",
  "keywords": [
    "WebdriverIO",
    "Cucumber"
  ],
  "author": "Mercer",
  "repository": "https://bitbucket.org/oliverwymantechssg/ngpd-merceros-testautomation-wdio-template",
  "license": "GPL-3.0",
  "dependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "chai-string": "^1.5.0",
    "deep-diff": "^1.0.2",
    "eslint": "^7.32.0",
    "exceljs": "^4.2.1",
    "fs-plus": "^3.1.1",
    "husky": "1.0.0-rc.13",
    "jsonfile": "6.1.0",
    "jsonpath": "^1.1.1",
    "jsonschema": "^1.4.0",
    "ngpd-merceros-testautomation-wdio-ta": ">=2.0.0",
    "lint-staged": "^11.1.1",
    "moment": "^2.29.1",
    "pixelmatch": "^5.2.1",
    "pngjs": "6.0.0",
    "request": "2.88.2",
    "semver": "^7.3.5",
    "xlsx": "^0.17.2",
    "yargs": "^17.2.1"
  },
  "scripts": {
    "lint": "eslint src/. --fix",
    "configure-globals": "npm run config/configure-globals.js",
    "test-local": "wdio wdio.local.conf.js --tag=@debug --instances=1",
    "test-setup": "wdio wdio.local.conf.js --tag=@example --instances=2",
    "test-sauceLab": "wdio wdio.sauce.conf.js",
    "test-local-mobile": "wdio wdio.local.conf.js --tag=@mobile --DEVICE_TYPE=mobile --DEVICE_NAME=\"Galaxy S5\" --HEADLESS=false --BROWSER_NAME=chrome",
    "test-local-api": "wdio wdio.local.conf.js --tag=@api --HEADLESS=true",
    "test-smoke": "wdio wdio.ci.conf.js --instances=3 --tag=@smoke",
    "test-smoke-headless": "wdio wdio.ci.conf.js --instances=3 --tag=@smoke --HEADLESS=true",
    "test-aws": "wdio wdio.conf.aws.js",
    "updateScreenshots": "node ./update-screenshots.js all",
    "allure:generate": "allure generate results -o ../locators-hotspots-chrome-plugin-master/allure-report --clean",
    "allure:open": "allure generate results -o results/allure-report --clean & allure open results/allure-report",
    "test-localization": "set LOCALIZATION=Chinese&&wdio wdio.local.conf.js",
    "pretest-localization": "set LOCALIZATION=Chinese&&node localizations-storing.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/register": "^7.15.3",
    "@wdio/allure-reporter": "^7.16.1",
    "@wdio/cli": "^7.16.1",
    "@wdio/cucumber-framework": "^7.16.1",
    "@wdio/devtools-service": "^7.16.12",
    "@wdio/local-runner": "^7.16.1",
    "@wdio/mocha-framework": "^7.16.1",
    "@wdio/sauce-service": "^7.16.1",
    "@wdio/selenium-standalone-service": "^7.16.1",
    "@wdio/spec-reporter": "^7.16.1",
    "@wdio/sync": "^7.16.1",
    "allure-commandline": "^2.13.8",
    "chromedriver": "94.0.0",
    "geckodriver": "2.0.4",
    "multiple-cucumber-html-reporter": "^1.18.0",
    "wdio-chromedriver-service": "7.2.2",
    "wdio-cucumberjs-json-reporter": "^4.1.4",
    "wdio-geckodriver-service": "^2.0.3",
    "wdio-intercept-service": "^4.1.7",
    "webdriverio": "^7.16.1"
  }
}